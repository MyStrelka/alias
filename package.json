{
  "name": "seaborn",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "server:build": "yarn workspace @seaborn/server run build",
    "server:dev": "yarn workspace @seaborn/server run dev",
    "server:start": "yarn workspace @seaborn/server run start",
    "client:build": "yarn workspace @seaborn/client run build",
    "client:dev": "yarn workspace @seaborn/client run dev",
    "packages:build": "yarn workspace @seaborn/shared run build"
  },
  "packageManager": "yarn@4.12.0",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "prettier": "^3.7.4"
  }
}
